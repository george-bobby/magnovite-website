{
	"trailingSlash": false,
	"cleanUrls": true,
	"rewrites": [
		{
			"source": "/",
			"destination": "/index.html"
		},
		{
			"source": "/about",
			"destination": "/about.html"
		},
		{
			"source": "/events",
			"destination": "/events.html"
		},
		{
			"source": "/gallery",
			"destination": "/gallery.html"
		}
	],
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			]
		},
		{
			"source": "/(.*).html",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=3600"
				}
			]
		}
	],
	"functions": {
		"src/**": {
			"includeFiles": "src/**"
		}
	}
}
